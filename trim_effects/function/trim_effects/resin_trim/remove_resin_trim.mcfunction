# Remove resin slowness.
attribute @s movement_speed modifier remove resin_one
attribute @s movement_speed modifier remove resin_two
attribute @s movement_speed modifier remove resin_three
attribute @s movement_speed modifier remove resin_four

# Remove resin block reach.
attribute @s block_interaction_range modifier remove resin_one
attribute @s block_interaction_range modifier remove resin_two
attribute @s block_interaction_range modifier remove resin_three
attribute @s block_interaction_range modifier remove resin_four

# Remove resin entity reach.
attribute @s entity_interaction_range modifier remove resin_one
attribute @s entity_interaction_range modifier remove resin_two
attribute @s entity_interaction_range modifier remove resin_three
attribute @s entity_interaction_range modifier remove resin_four

# Remove resin jump strength.
attribute @s jump_strength modifier remove resin_one
attribute @s jump_strength modifier remove resin_two
attribute @s jump_strength modifier remove resin_three
attribute @s jump_strength modifier remove resin_four

# Remove resin knockback resistance.
attribute @s knockback_resistance modifier remove resin_one
attribute @s knockback_resistance modifier remove resin_two
attribute @s knockback_resistance modifier remove resin_three
attribute @s knockback_resistance modifier remove resin_four

# Remove resin_trim tag
tag @s remove resin_trim

